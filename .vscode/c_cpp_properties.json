{
    "configurations": [
        {
            "name": "AVR",
            "intelliSenseMode": "clang-x64",
            "compilerPath": "/usr/bin/avr-gcc",
            "includePath": [
                "${workspaceFolder}",
                "${default}"
            ],
            "forcedInclude": [ "${workspaceFolder}/lib/ATmega_DFP/include/avr/iom4809.h" ],
            "defines": ["__AVR_ATmega4809__","F_CPU = 20000000UL/6"],
            "cStandard": "c11",
            "cppStandard": "c++17",
            "browse": {
                "path": [ "${workspaceFolder}" ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}